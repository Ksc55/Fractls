#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@graphprotocol/graph-cli/node_modules/assemblyscript/bin/asinit" "$@"
  ret=$?
else 
  node  "$basedir/../@graphprotocol/graph-cli/node_modules/assemblyscript/bin/asinit" "$@"
  ret=$?
fi
exit $ret
